{
  "name": "github-api-demo",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "./dist/server.js",
    "build": "tsc --project tsconfig.build.json",
    "watch": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'ts-node src/server.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --fix \"**/*.ts\"",
    "lint:staged": "lint-staged",
    "prepare": "husky install"
  },
  "repository": "git@github.com:ammuench/github-api-demo.git",
  "author": "Alex Muench <ammuench@gmail.com>",
  "license": "Private",
  "dependencies": {
    "axios": "^1.3.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-amuench": "^1.0.0",
    "husky": "^8.0.0",
    "jest": "^29.4.2",
    "lint-staged": "^13.1.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  }
}
