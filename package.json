  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "tsc --build",
    "watch": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'ts-node src/server.ts'",
