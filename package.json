  "author": "Alex Muench <ammuench@gmail.com>",
  "license": "Private",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-amuench": "^1.0.0",
    "husky": "^8.0.0",
    "jest": "^29.4.2",
    "lint-staged": "^13.1.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  }
